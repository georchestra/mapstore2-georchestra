{
  "name": "georchestra",
  "version": "1.5.0",
  "description": "geOrchestra",
  "repository": "https://github.com/georchestra/mapstore2-georchestra",
  "main": "index.js",
  "eslintConfig": {
    "extends": [
      "@mapstore/eslint-config-mapstore"
    ],
    "parserOptions": {
      "babelOptions": {
        "configFile": "./MapStore2/build/babel.config.js"
      }
    }
  },
  "eslintIgnore": [
    "MapStore2"
  ],
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all",
      "not IE 11",
      "not UCAndroid 12.12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@geosolutions/acorn-jsx": "4.0.2",
    "@geosolutions/jsdoc": "3.4.4",
    "@geosolutions/mocha": "6.2.1-3",
    "@mapstore/eslint-config-mapstore": "1.0.6",
    "@testing-library/react": "12.1.5",
    "axios-mock-adapter": "2.1.0",
    "babel-loader": "10.0.0",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-transform-imports": "2.0.0",
    "copy-webpack-plugin": "5.0.2",
    "css-loader": "5.1.2",
    "css-minimizer-webpack-plugin": "3.0.2",
    "denodeify": "1.2.1",
    "docma": "1.5.1",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-no-only-tests": "2.6.0",
    "eslint-plugin-react": "3.16.1",
    "expect": "1.20.1",
    "file-loader": "2.0.0",
    "glob": "7.1.1",
    "html-loader": "2.0.0",
    "html-webpack-plugin": "5.2.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-cli": "2.0.0",
    "karma-coverage": "2.2.0",
    "karma-coveralls": "2.1.0",
    "karma-firefox-launcher": "2.1.2",
    "karma-junit-reporter": "2.0.1",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.8",
    "karma-webpack": "5.0.0",
    "less": "4.1.1",
    "less-loader": "8.0.0",
    "markdownlint-cli": "0.34.0",
    "mini-css-extract-plugin": "1.3.9",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "postcss": "8.3.5",
    "postcss-loader": "6.1.0",
    "postcss-prefix-selector": "1.10.0",
    "premove": "4.0.0",
    "prettier": "3.5.1",
    "process": "0.11.10",
    "raw-loader": "0.5.1",
    "react-transition-group": "1.1.3",
    "readline-promise": "1.0.4",
    "redux-devtools": "3.4.0",
    "redux-devtools-dock-monitor": "1.1.2",
    "redux-devtools-log-monitor": "1.3.0",
    "redux-mock-store": "1.2.2",
    "simple-git": "2.20.1",
    "style-loader": "2.0.0",
    "terser": "5.18.1",
    "url-loader": "0.5.7",
    "webpack": "5.94.0",
    "webpack-bundle-size-analyzer": "2.0.2",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "mapstore": "file:MapStore2"
  },
  "scripts": {
    "start": "npm run app:start",
    "app:start": "concurrently -n frontend,backend -c green,blue \"npm:fe:start\" \"npm:be:start\"",
    "fe:start": "webpack serve --progress --color --port 8081 --hot --inline --content-base .",
    "fe:build": "npm run clean && mkdirp ./dist && node ./node_modules/webpack/bin/webpack.js --config prod-webpack.config.js",
    "compile": "npm run fe:build",
    "backend:build": "mvn clean install -Pprinting",
    "be:start": "npm run backend:build -Pprinting && mvn cargo:run -f ./web/pom.xml -Pprinting",
    "clean": "rimraf dist",
    "postinstall": "node MapStore2/utility/build/postInstall.js",
    "test": "karma start ./karma.conf.single-run.js",
    "continuoustest": "karma start ./karma.conf.continuous-test.js",
    "mvntest": "karma start ./karma.conf.single-run.js --reporters junit,dots,coverage",
    "backend:dev": "npm run be:start",
    "lint": "eslint js --ext .jsx,.js",
    "updateDevDeps": "node updateDevDependencies.js"
  },
  "author": "GeoSolutions",
  "license": "BSD-2-Clause"
}
